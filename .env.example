# Enable or disable running the MCP server
RUN_MCP_SERVER=true

# LLM provider (anthropic, openai, mistral, gemini, ollama)
LM_PROVIDER=anthropic

# Default model name for the LLM
LM_MODEL=claude-3-5-sonnet-20241022

# LLM provider API key for this session
LM_TOKEN=

# OCL user token for authenticated requests
OCL_TOKEN=

# OCL API URL preset: production, staging, local
OCL_URL=production

# Alternatively, set a full custom OCL API URL
# OCL_URL=https://api.openconceptlab.org/
